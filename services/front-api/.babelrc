{
  "presets": ["env"],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["module-resolver", {
      "root": ["./build"],
      "alias": {
        "conf": "./build/conf",
        "controllers": "./build/controllers",
        "middlewares": "./build/middlewares",
        "routes": "./build/routes",
        "helpers": "./build/helpers",
        "services": "./build/services"
      }
    }]
  ]
}
